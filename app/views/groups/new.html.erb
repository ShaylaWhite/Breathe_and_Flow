<h1>New Groups</h1>

<%= form_for :group do |f| %>

    <%= f.label :name %>
    <%=  f.text_field :name %>
       <br>
       <br>
    <%= f.label :description %>
    <%= f.text_field :description %>
           <br>
           <br>
    <%= f.label :time %>
    <%= f.time_field :time %>
           <br>
           <br>

    <%= f.submit "Submit" %>

<% end %>